<?xml version='1.0' encoding='utf-8'?>
<widget id="web.microzaim.online" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Микрозаймы</name>
    <description>
        Подбор онлайн микрозаймов в России
    </description>
    <author email="admin@microzaim.info" href="https://Microzaim.info">
        Microcredit
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="26" />
</widget>
